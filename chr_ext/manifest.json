{
  "manifest_version": 2,

  "name": "Tomato",
  "description": "This extension shows dotes",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "dotes.html",
    "default_title": "Woohoo!"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
